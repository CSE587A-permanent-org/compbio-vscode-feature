{
  "id": "compbio-vscode-customizations",
  "version": "1.0.0",
  "name": "CompBio VS Code Customizations",
  "documentationURL": "https://github.com/yourusername/compbio-vscode-feature",
  "options": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "njpwerner.autodocstring",
        "ms-python.mypy-type-checker"
      ],
      "settings": {
        "python.pythonPath": "${workspaceFolder}/.venv/bin/python"
      }
    }
  }
}